<COMPONENT>

     <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
     EXTENSION="org.talend.designer.dbmap"
     SCHEMA_AUTO_PROPAGATE="false"
     DATA_AUTO_PROPAGATE="false"
     HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>ELT/Map/MySQL</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
<!--		<CONNECTOR CTYPE="FLOW" BUILTIN="true" MIN_OUTPUT="1" MAX_INPUT="0"/> -->
	    <CONNECTOR CTYPE="TABLE" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1"/>
	    <CONNECTOR CTYPE="TABLE_REF" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1" />

		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
         NAME="REFERENCE"
         SHOW="false"
         FIELD="CHECK"
         NUM_ROW="1">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

  	<PARAMETER
		NAME="DB_VERSION"
		FIELD="CLOSED_LIST"
		NUM_ROW="1"
		REPOSITORY_VALUE="DB_VERSION"
		SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'false')">
		<ITEMS DEFAULT="MYSQL_5">
			<ITEM NAME="MYSQL_5" VALUE="MYSQL_5" />
			<ITEM NAME="MYSQL_4" VALUE="MYSQL_4" />
		</ITEMS>
	</PARAMETER>

	   <PARAMETER
	     NAME="USE_EXISTING_CONNECTION"
	     FIELD="CHECK"
	     NUM_ROW="2"
	     SHOW_IF="REFERENCE == 'false'"
	   >
	     <DEFAULT>false</DEFAULT>
	   </PARAMETER>

	   <PARAMETER
	     NAME="CONNECTION"
	     FIELD="COMPONENT_LIST"
	     REQUIRED="true" FILTER="tMysqlConnection"
	     NUM_ROW="2"
	     SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'true')"
	     DYNAMIC_SETTINGS="true"/>

      <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="10">
         <DEFAULT/>
      </PARAMETER>

      <PARAMETER
        NAME="LINK_STYLE"
        FIELD="CLOSED_LIST"
        NUM_ROW="10"
        SHOW_IF="REFERENCE == 'false'"
      >
        <ITEMS DEFAULT="">
          <ITEM NAME="AUTO"   VALUE="AUTO" />
          <ITEM NAME="BEZIER_CURVE"   VALUE="BEZIER_CURVE" />
          <ITEM NAME="LINE"   VALUE="LINE" />
        </ITEMS>
      </PARAMETER>


    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="20"
      REPOSITORY_VALUE="DATABASE"
      SHOW_IF="REFERENCE == 'false'"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>"3306"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="40"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="(REFERENCE == 'false') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="50"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="50"
      SHOW="true"
      READONLY="true"
      SHOW_IF="REFERENCE == 'false'"
    >
      <DEFAULT>mysql_id</DEFAULT>
    </PARAMETER>


      <PARAMETER NAME="PREVIEW" FIELD="IMAGE" SHOW="true" NUM_ROW="60" SHOW_IF="REFERENCE == 'false'">
         <DEFAULT/>
      </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="MYSQL_5" MODULE="mysql-connector-java-5.1.30-bin.jar" REQUIRED_IF="(DB_VERSION == 'MYSQL_5') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="MYSQL_4" MODULE="mysql-connector-java-3.1.14-bin.jar" REQUIRED_IF="(DB_VERSION == 'MYSQL_4') AND (USE_EXISTING_CONNECTION == 'false')" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
   </RETURNS>

   <PLUGINDEPENDENCIES>
   	<PLUGINDEPENDENCY ID="org.talend.designer.abstractmap" />
  <!-- 	<PLUGINDEPENDENCY ID="org.talend.designer.dbmap" />  -->
   </PLUGINDEPENDENCIES>
</COMPONENT>
