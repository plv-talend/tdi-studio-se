<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20090713A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>
  <FAMILIES>
    <FAMILY>Databases/Vertica</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:VERTICA"
    />

	<PARAMETER
		NAME="DB_VERSION"
		FIELD="CLOSED_LIST"
		NUM_ROW="10"
		REPOSITORY_VALUE="DB_VERSION">
		<ITEMS DEFAULT="VERTICA_3">
			<ITEM NAME="VERTICA_3" VALUE="vertica_3.0_jdk_5.jar" />
			<ITEM NAME="VERTICA_3.5" VALUE="vertica_3.5_jdk_5.jar" />
			<ITEM NAME="VERTICA_4" VALUE="vertica_4.0_jdk_5.jar" />
			<ITEM NAME="VERTICA_4.1" VALUE="vertica_4.1.7_jdk_5.jar" />
			<ITEM NAME="VERTICA_5.0" VALUE="vertica_4.1.14_jdk_5.jar" />
			<ITEM NAME="VERTICA_5_1" VALUE="VERTICA_5_1" />
			<ITEM NAME="VERTICA_6_0" VALUE="VERTICA_6_0" />
			<ITEM NAME="VERTICA_6_1_X" VALUE="VERTICA_6_1_X" />
			<ITEM NAME="VERTICA_7_0_X" VALUE="VERTICA_7_0_X" />
		</ITEMS>
	</PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>VERTICA</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
    >
      <DEFAULT>"5433"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
		NAME="SCHEMA_DB"
		FIELD="TEXT"
		NUM_ROW="30"
		REPOSITORY_VALUE="SCHEMA">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="40"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="50"
		REPOSITORY_VALUE="PROPERTIES_STRING">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="80"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="USE_SHARED_CONNECTION"
	  FIELD="CHECK"
      NUM_ROW="90"
      REQUIRED="true"
    >
    </PARAMETER>

	<PARAMETER
      NAME="SHARED_CONNECTION_NAME"
	  FIELD="TEXT"
      NUM_ROW="90"
      REQUIRED="true"
	  SHOW_IF="USE_SHARED_CONNECTION == 'true'"
    >
	<DEFAULT></DEFAULT>
    </PARAMETER>
   </PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="AUTO_COMMIT" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
    	</PARAMETER>
	</ADVANCED_PARAMETERS>

   <CODEGENERATION>
       <IMPORTS>
		<IMPORT NAME="Driver-VERTICA" MODULE="vertica_3.0_jdk_5.jar"  REQUIRED="false" />
		<IMPORT NAME="Driver-VERTICA_3.5" MODULE="vertica_3.5_jdk_5.jar"  REQUIRED="false" />
		<IMPORT NAME="Driver-VERTICA_4" MODULE="vertica_4.0_jdk_5.jar"  REQUIRED="false" />
		<IMPORT NAME="Driver-VERTICA_4.1" MODULE="vertica_4.1.7_jdk_5.jar"  REQUIRED="false" />
		<IMPORT NAME="Driver-VERTICA_5.0" MODULE="vertica_4.1.14_jdk_5.jar"  REQUIRED="false" />
		<IMPORT NAME="Driver-VERTICA_5.1" MODULE="vertica_5.1.6_jdk_5.jar"  REQUIRED_IF="DB_VERSION=='VERTICA_5_1'" />
		<IMPORT NAME="Driver-VERTICA_6.0" MODULE="vertica-jdk5-6.0.2-0.jar"  REQUIRED_IF="DB_VERSION=='VERTICA_6_0'" />
		<IMPORT NAME="Driver-VERTICA_6.1.2" MODULE="vertica-jdk5-6.1.2-0.jar"  REQUIRED_IF="DB_VERSION=='VERTICA_6_1_X'" />
		<IMPORT NAME="Driver-VERTICA_7.0" MODULE="vertica-jdbc-7.0.1-0.jar"  REQUIRED_IF="DB_VERSION=='VERTICA_7_0_X'" />
    </IMPORTS>
  </CODEGENERATION>

   <RETURNS/>
</COMPONENT>
